<!-- datatbles buttons bundle contains: 
	+ jszip.js (needed to generate zipped files for CVS download)
	+ pdfmake.js (needed to generate PDF files)
	+ vfs_fonts.js (depedency for pdfmake.js)
	+ dataTables.buttons.js (core button generation file for datatables)
	+ buttons.bootstrap4.js (optional BS4 style)
	+ buttons.colVis.js (needed to toggle column visibility)
	+ buttons.html5.js (dependency for datatables.buttons.js)
	+ buttons.print.js (needed to generate print format )

	NOTE: 	The file size is pretty big, but you can always use the
			build.json file to deselect any components you do not need) -->
<script src="js/datagrid/datatables/datatables.buttons.bundle.js"></script>

<script>
	$(document).ready( function () {
		//reset the default classes
		$.extend( $.fn.dataTableExt.oStdClasses, {
			"sFilterInput": "form-control border-top-left-radius-0 border-bottom-left-radius-0 ml-0 width-lg shadow-inset-1",
			"sLengthSelect": "form-control custom-select"
		});

		// initialize datatable
		$('#dt-basic-example').dataTable({
			responsive: true,
			dom: 
				"<'row'<'col-sm-12 col-md-6'f><'col-sm-12 col-md-6 text-right'B>>" +
				"<'row'<'col-sm-12'tr>>" +
				"<'row'<'col-sm-12 col-md-5'i><'col-sm-12 col-md-7'p>>",
			buttons: [
				{
					extend:    'pdfHtml5',
					text:      '<i class="fal fa-file-pdf text-danger fs-lg"></i><span class="hidden-sm-down ml-2">PDF</span>',
					titleAttr: 'PDF',
					className: 'btn btn-outline-default shadow-0'
				},
				{
					extend:    'excelHtml5',
					text:      '<i class="fal fa-file-excel color-success-700 fs-lg"></i><span class="hidden-sm-down ml-2">Excel</span>',
					titleAttr: 'Excel',
					className: 'btn btn-outline-default shadow-0'
				},
				{
					extend:    'csvHtml5',
					text:      '<i class="fal fa-file-archive color-success-700 fs-lg"></i><span class="hidden-sm-down ml-2">CSV</span>',
					titleAttr: 'CSV',
					className: 'btn btn-outline-default shadow-0'
				},
				{
					extend:    'copyHtml5',
					text:      '<i class="fal fa-copy color-primary-500 fs-xl d-block hidden-sm-up"></i><span class="hidden-sm-down">Copy</span>',
					titleAttr: 'Copy',
					className: 'btn btn-outline-default shadow-0'
				},
				{
					extend:    'print',
					text:      '<i class="fal fa-file-archive color-success-700 fs-xl d-block hidden-sm-up"></i><span class="hidden-sm-down">Print</span>',
					titleAttr: 'Print',
					className: 'btn btn-outline-default shadow-0'
				}/*,
				{
					extend:    'colvis',
					text:      '<i class="fal fa-eye color-info-500 fs-xl d-block hidden-sm-up"></i><span class="hidden-sm-down">Visibility</span>',
					titleAttr: 'Col visibility',
					className: 'btn btn-outline-default shadow-0'
				}*/
			],			
			language: {
				//change the default text for 'next' and 'previous' with icons
				paginate: {
					previous: "<i class='fal fa-chevron-left'></i>",
					next: "<i class='fal fa-chevron-right'></i>"
				},
				//replace the default search lable text with a nice icon
				search: '<div class="d-inline-flex width-3 align-items-center justify-content-center border form-control border-bottom-right-radius-0 border-top-right-radius-0 border-right-0 btn-default"><i class="fal fa-search"></i></div>',
				//add search filter
				searchPlaceholder: "Filter...",
				//change text for zero records 
				zeroRecords: "No records to display"					
			}
		});

	});
</script>

