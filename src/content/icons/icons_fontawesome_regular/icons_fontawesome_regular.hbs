<script>
			
	initApp.listFilter($('#fontawesome-pro'), $('#filter-icon'));

	$('.js-showcase-icon').click(function(){

		var iconClass = $(this).find('i').attr('class');
		
		$('#exampleModal .modal-body i').removeClass().addClass(iconClass);
		$('#exampleModal .modal-body .js-icon-class').empty().text(iconClass);
		$('#js-icon-class').val('<i class="' + iconClass + '"></i>')

		//document.execCommand('copy')
		
	});

	$('.js-icon-copy').click(function(){

		$('#js-icon-class').select();
		document.execCommand('copy');
	})

</script>

